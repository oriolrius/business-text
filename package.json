    "@grafana/ui": "^12.0.2",
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^14.1.2",
    "@volkovlabs/components": "^4.4.0",
    "dayjs": "^1.11.13",
    "handlebars": "^4.7.8",
    "helper-date": "^1.0.1",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.2",
